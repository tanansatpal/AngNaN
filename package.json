{
  "name": "ang-nan",
  "version": "0.0.0",
  "scripts": {
    "ng": "./node_modules/@angular/cli/bin/ng",
    "start": "ng serve --aot --host 0.0.0.0",
    "build": "ng build --aot",
    "build-prod": "ng build --aot --prod",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "bundle-report": "ng build --aot --prod --stats-json && webpack-bundle-analyzer dist/browser/stats.json",
    "serve:ssr": "node dist/server.js",
    "build:client-and-server-bundles": "ng build --prod && ng run AngNaN:server",
    "webpack:server": "./node_modules/webpack/bin/webpack.js --config webpack.server.config.js --progress --colors",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compodoc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json -d docs/",
    "format:fix": "pretty-quick --staged",
    "firebase-deploy": "firebase deploy"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s format:fix lint"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.3",
    "@angular/common": "^8.2.3",
    "@angular/compiler": "^8.2.3",
    "@angular/core": "^8.2.3",
    "@angular/forms": "^8.2.3",
    "@angular/http": "^7.0.0",
    "@angular/platform-browser": "^8.2.3",
    "@angular/platform-browser-dynamic": "^8.2.3",
    "@angular/platform-server": "^8.2.3",
    "@angular/pwa": "^0.10.3",
    "@angular/router": "^8.2.3",
    "@angular/service-worker": "^8.2.3",
    "@fortawesome/angular-fontawesome": "^0.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.7",
    "@fortawesome/free-brands-svg-icons": "^5.6.3",
    "@fortawesome/free-solid-svg-icons": "^5.4.2",
    "@ngrx/effects": "^7.0.0",
    "@ngrx/store": "^7.0.0",
    "@ngrx/store-devtools": "^7.2.0",
    "@ngu/carousel": "^1.5.5",
    "@nguniversal/module-map-ngfactory-loader": "^7.0.2",
    "add": "^2.0.6",
    "bootstrap": "^4.1.3",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "jquery": "^3.3.1",
    "ngx-bootstrap": "^5.2.0",
    "ngx-owl-carousel": "^2.0.7",
    "ngx-pagination": "^3.2.1",
    "node-sass": "^4.11.0",
    "owl.carousel": "^2.3.4",
    "popper.js": "^1.14.4",
    "preboot": "^7.0.0",
    "rxjs": "~6.5.2",
    "subsink": "^1.0.0",
    "ts-loader": "^6.0.0",
    "yarn": "^1.10.1",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.0",
    "@angular/cli": "~8.3.0",
    "@angular/compiler-cli": "^8.2.3",
    "@angular/language-service": "^8.2.3",
    "@compodoc/compodoc": "^1.1.8",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "husky": "^1.3.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.2.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "protractor": "~5.4.0",
    "ts-node": "^7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.5.3",
    "webpack-cli": "^3.1.2"
  }
}
